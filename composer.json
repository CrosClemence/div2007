{
  "name": "jambagecom/div2007",
  "type": "typo3-cms-extension",
  "description": "Static Methods since 2007 for TYPO3 CMS. It offers many functions for Front End extensions. You can replace t3lib_div and tslib_pibase by div2007.",
  "keywords": [
    "TYPO3",
    "extension",
    "library",
    "t3lib_div",
    "tslib_pibase",
    "static_info_tables",
    "t3skin"
  ],
  "support": {
    "issues": "https://github.com/franzholz/div2007/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "typo3/cms-core": "^10.4 || ^11.5 || ^12.2",
    "typo3/cms-reports": "^10.4 || ^11.5 || ^12.2"    
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\Div2007\\": "Classes"
    },
    "classmap": [
      "/",
      "hooks/",
      "lib/",
      "spl/"
    ]
  },
  "replace": {
    "typo3-ter/div2007": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "div2007"
    }
  }
}
